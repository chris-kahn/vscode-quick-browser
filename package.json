{
    "name": "tiller",
    "displayName": "Tiller",
    "repository": {
        "type": "git",
        "url": "https://github.com/chris-kahn/vscode-tiller"
    },
    "description": "A Helm-like file browser for VS Code",
    "version": "1.0.5",
    "publisher": "kahnpro",
    "engines": {
        "vscode": "^1.48.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:tiller.show"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "tiller.show",
                "title": "Tiller"
            },
            {
                "command": "tiller.back",
                "title": "Tiller - Up a directory"
            },
            {
                "command": "tiller.forward",
                "title": "Tiller - Open directory or file"
            },
            {
                "command": "tiller.toggleHidden",
                "title": "Tiller - Toggle Hidden Files"
            }
        ],
        "keybindings": [
            {
                "command": "tiller.show",
                "key": "alt+p",
                "mac": "ctrl+p"
            },
            {
                "command": "tiller.back",
                "key": "left",
                "mac": "left"
            },
            {
                "command": "tiller.forward",
                "key": "right",
                "mac": "right"
            },
            {
                "command": "tiller.toggleHidden",
                "key": "alt+.",
                "mac": "ctrl+."
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "test": "npm run compile && node ./out/test/index.js"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^13.13.0",
        "@types/vscode": "^1.48.0",
        "tslint": "^6.1.3",
        "typescript": "^4.0.2",
        "vscode-test": "^1.4.0"
    }
}