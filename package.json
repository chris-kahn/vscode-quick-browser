{
    "name": "quick-browser",
    "displayName": "Quick Browser",
    "repository": {
        "type": "git",
        "url": "https://github.com/chris-kahn/vscode-quick-browser"
    },
    "description": "A Helm-like file browser for VS Code",
    "version": "1.1.0",
    "publisher": "kahnpro",
    "engines": {
        "vscode": "^1.33.0"
    },
    "categories": ["Other"],
    "activationEvents": ["onCommand:quick-browser.show"],
    "license": "GPL-3.0-only",
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "quick-browser.show",
                "title": "Quick Browser"
            },
            {
                "command": "quick-browser.back",
                "title": "Quick Browser - Up a directory"
            },
            {
                "command": "quick-browser.forward",
                "title": "Quick Browser - Open directory or file"
            },
            {
                "command": "quick-browser.toggleHidden",
                "title": "Quick Browser - Toggle Hidden Files"
            }
        ],
        "keybindings": [
            {
                "command": "quick-browser.show",
                "key": "alt+p",
                "mac": "ctrl+p"
            },
            {
                "command": "quick-browser.back",
                "key": "alt+left",
                "mac": "ctrl+left"
            },
            {
                "command": "quick-browser.forward",
                "key": "alt+right",
                "mac": "ctrl+right"
            },
            {
                "command": "quick-browser.toggleHidden",
                "key": "alt+.",
                "mac": "ctrl+."
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.6",
        "@types/node": "^10.12.21",
        "@types/vscode": "^1.33.0",
        "glob": "^7.1.4",
        "mocha": "^6.1.4",
        "typescript": "^3.3.1",
        "tslint": "^5.12.1",
        "vscode-test": "^1.0.2"
    }
}
